{% include '@app/header.twig' %}<div class="col-md-12 col-sm-12 col-xs-12">    <div class="x_panel">        <div class="x_title">            <h2>                {% if not empty(shipping.id) %}                    {% helper lang.l('admin_header_shipping_change', 'store') %}                {% else %}                    {% helper lang.l('admin_header_shipping_add', 'store') %}                {% endif %}            </h2>            <div class="clearfix"></div>        </div>        <div class="x_content">            <form method="post" name="save_form" enctype="multipart/form-data"                data-parsley-validate class="form-horizontal form-label-left">                <div class="form-group">                    <label class="control-label col-md-3 col-sm-3 col-xs-12">                        {% helper lang.l('field_shipping_name', 'store') %}:&nbsp;*                    </label>                    <div class="col-md-9 col-sm-9 col-xs-12">                        {% for lang_id, lang_item  in langs %}                            {% set name = 'name_' ~ lang_id %}                            {% if lang_id == current_lang_id %}                                <input type="text" name="name_{{ lang_id }}" value="{{ shipping[name]|escape }}" class="form-control"                                   lang-editor="value" lang-editor-type="data-name" lang-editor-lid="{{ lang_id }}" />                            {% else %}                                <input type="hidden" name="name_{{ lang_id }}" value="{{ shipping[name]|escape }}" class="form-control"                                   lang-editor="value" lang-editor-type="data-name" lang-editor-lid="{{ lang_id }}" />                            {% endif %}                        {% endfor %}                    </div>                    <div class="accordion col-md-9 col-sm-9 col-xs-12 col-md-offset-3 col-sm-offset-3" id="accordion" role="tablist" aria-multiselectable="true">                        <div class="panel">                            <a class="panel-heading" role="tab" id="headingOne" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">                                <h4 class="panel-title">{% helper lang.l('others_languages', 'start') %}</h4>                            </a>                            <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">                                <div class="panel-body">                                    {% for lang_id, item in langs %}                                        {% set name = 'name_' ~ lang_id %}                                        {% if lang_id != current_lang_id %}                                        <div class="form-group">                                            <label class="control-label col-md-2 col-sm-2 col-xs-12">{{ item.name }}</label>                                            <div class="col-md-10 col-sm-10 col-xs-12">                                                <input type="text" name="name_{{ lang_id }}" class="form-control"                                                       value="{{ shipping[name]|escape }}">                                            </div>                                        </div>                                        {% endif %}                                    {% endfor %}                                </div>                            </div>                        </div>                    </div>                </div>                <div class="form-group">                    <label class="control-label col-sm-3 col-xs-12">                      {% helper lang.l('field_locations', 'store') %}:&nbsp;*</label>                    <div class="col-sm-9 col-xs-12">                        <a href="#" id="country_open" class="btn btn-default">{% helper lang.l('link_select_region', 'countries') %}</a>                        <span id="block_countries">{% helper store.store_countries_select({id_shipping: shipping.id}) %}</span>                    </div>                </div>                <div class="form-group">                    <label class="control-label col-sm-3 col-xs-12">                      {% helper lang.l('field_shipping_price', 'store') %} ({% helper start.currency_format_output() %}):</label>                    <div class="col-sm-9 col-xs-12">                        <input type="text" name="price" value="{{ shipping.price|escape }}" class="form-control">                    </div>                </div>                <div class="form-group">                    <label class="control-label col-sm-3 col-xs-12">                      {% helper lang.l('field_shipping_description', 'store') %}:</label>                    <div class="col-md-9 col-sm-9 col-xs-12">                        {% for lang_id, lang_item in langs %}                            {% set description = 'description_' ~ lang_id %}                            {% if lang_id == current_lang_id %}                                <textarea name="description_{{ lang_id }}" class="form-control">{{ shipping[description]|escape }}</textarea>                            {% else %}                                <input type="hidden" name="description_{{ lang_id }}" value="{{ shipping[description]|escape }}" class="form-control">                            {% endif %}                        {% endfor %}                    </div>                    <div class="accordion col-md-9 col-sm-9 col-xs-12 col-md-offset-3 col-sm-offset-3" id="accordion" role="tablist" aria-multiselectable="true">                        <div class="panel">                            <a class="panel-heading" role="tab" id="headingTwo" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">                                <h4 class="panel-title">{% helper lang.l('others_languages', 'start') %}</h4>                            </a>                            <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">                                <div class="panel-body">                                    {% for lang_id, lang_item in langs %}                                        {% set description = 'description_' ~ lang_id %}                                        {% if lang_id != current_lang_id %}                                        <div class="form-group">                                            <label class="control-label col-md-2 col-sm-2 col-xs-12">{{ lang_item.name }}</label>                                            <div class="col-md-10 col-sm-10 col-xs-12">                                                <textarea name="description_{{ lang_id }}" class="form-control">{{ shipping[description]|escape }}</textarea>                                            </div>                                        </div>                                        {% endif %}                                    {% endfor %}                                </div>                            </div>                        </div>                    </div>                </div>                <div class="ln_solid"></div>                <div class="form-group">                    <div class="col-sm-9 col-xs-12 col-sm-offset-3">                        <input class="btn btn-success" type="submit" name="btn_save" value="{% helper lang.l('btn_save', 'start', "", 'button') %}">                        <a class="btn btn-default" href="{{ back_url }}">{% helper lang.l('btn_cancel', 'start') %}</a>                    </div>                </div>            </form>        </div>    </div>    <div class="clearfix"></div><script type='text/javascript'>    $(function(){        loadScripts(            ["{% helper utils.jscript('store', 'admin-store.js', 'path') %}"],            function(){                admin_store = new AdminStore;            },            ['admin_store'],            {async: true}        );        country_open = new loadingContent({            loadBlockSize: 'big',            loadBlockTitle: '{% helper lang.l('field_countries', 'store') %}',            footerButtons: '<input class="btn btn-success" type="button" value="{% helper lang.l('btn_select', 'store') %}" name="btn_save" id="submit_loc">'        });        $('#country_open').off('click').click(function(){            $.ajax({                url: site_url + 'admin/store/ajax_country_select/{{ shipping.id }}',                cache: false,                success: function(data){                    country_open.show_load_block(data);                }            });            return false;        });    });</script>{% include '@app/footer.twig' %}