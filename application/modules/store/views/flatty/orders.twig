{% include '@app/header.twig' %}<div class="store-content">    <div class="container">        <div class="row-content">            <div class="search-header">                <div class="title col-xs-12 col-sm-10 col-md-10 col-lg-10 no-padding-left">                    {% helper seo.seo_tags('header_text') %}:&nbsp;#{{ orders.code }} ({{ orders.status_text }})                </div>                <div class="link-order_log col-xs-12 col-sm-2 col-md-2 col-lg-2">                    <button type="button" class="btn btn-default btn-xs pull-right">                        <i class="fa fa-angle-double-down"></i>&nbsp;{% helper lang.l('link_order_log', 'store') %}                    </button>                </div>            </div>            <div class="clr"></div>            <div id="order_log_block_{{ orders.id }}" class="col-xs-12 col-lg-12 order-log-block clearfix"></div>            <div class="clr"></div>            <div class="order-main-block">                <div class="contrasting-block clearfix">                    <div class="cart-header-block">                        <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8 no-padding-left">{% helper lang.l('field_product_description', 'store') %}</div>                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{% helper lang.l('field_count', 'store') %}</div>                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{% helper lang.l('field_product_price', 'store') %}</div>                    </div>                    <div class="cart-products">                        {% for item in orders.items %}                           <div class="cart-product-item clearfix">                                <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8 no-padding-left">                                    <div class="product-image">                                        {% if item.product.media.mediafile.thumbs.middle is not empty %}                                            <img src="{{ item.product.media.mediafile.thumbs.middle }}"                                                  alt="{{ item.product.name|e }}" title="{{ item.product.name|e }}" class="img-rounded img-responsive">                                        {% else %}                                            <div class="default-middle-image">                                                <i class="fa fa-gift fa-4x"></i>                                            </div>                                        {% endif %}                                    </div>                                    <div class="product-description">{{ item.product.name }}                                        {% for opt in item.product.options %}                                            {% set option_id = opt.id %}                                            <div>                                                {% helper store.store_options_values({                                                    'header': opt.name,                                                    'id': opt.id,                                                    'options': item.options[option_id],                                                    'action': 'view',                                                    'source': 'user'                                                }) %}                                            </div>                                        {% endfor %}                                        <div class="preorder-item-title">{% helper lang.l('field_count', 'store') %}:&nbsp;{{ item.count }}</div>                                        <div class="preorder-item-title">{% helper lang.l('field_product_price', 'store') %}:&nbsp;{% helper start.currency_format_output({'value': item.price}) %}</div>                                    </div>                                </div>                                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 hide-in-mobile">                                    {{ item.count }}                                </div>                                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 hide-in-mobile">                                   {% helper start.currency_format_output({'value': item.price}) %}                                </div>                            </div>                        {% else %}                            <div class="cart-product-item clearfix">                                {% helper lang.l('field_cart_empty', 'store') %}                            </div>                        {% endfor %}                        <div class="cart-total-line">                            <div class="col-xs-12 no-padding-right">                                {% helper lang.l('field_total_with_delivery', 'store') %}                                 ({{ orders.products_count }}&nbsp;{% helper lang.l('field_pc', 'store') %}):&nbsp;                                {% helper start.currency_format_output({'value': orders.total}) %}                            </div>                        </div>                    </div>                </div>            </div>            <div class="order-items-descr mt20 p10">                {% if orders.user %}                    <div>                        {% helper lang.l('field_shipping_contact_name', 'store') %}:&nbsp;                        {{ orders.user }}                    </div>                {% endif %}                {% if orders.id_customer == orders.id_user %}                    {% if orders.shipping_location %}                        <div>                            {% helper lang.l('field_street_address', 'store') %}:&nbsp;                            {{ orders.shipping_location }}                        </div>                    {% endif %}                    {% if orders.shipping_zip %}                        <div>                            {% helper lang.l('field_zip', 'store') %}:&nbsp;                            {{ orders.shipping_zip }}                        </div>                    {% endif %}                    {% if orders.contact_phone %}                        <div>                            {% helper lang.l('field_phone', 'store') %}:&nbsp;                            {{ orders.contact_phone }}                        </div>                    {% endif %}                    {% if orders.comment %}                        <div>                            {% helper lang.l('field_comment', 'store') %}:&nbsp;                            {{ orders.comment }}                        </div>                    {% endif %}                {% endif %}            </div>        </div>    </div></div><script>    $(function(){        loadScripts(            "{% helper utils.jscript('store', 'store_orders.js', 'path') %}",            function(){                orders = new storeOrders({                    siteUrl: site_url,                    order: {id_order: '{{ orders.id }}'},                });            },            'orders',            {async: true}        );    });</script>{% include '@app/footer.twig' %}
