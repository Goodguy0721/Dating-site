{% include '@app/header.twig' %}<div class="content-block">	<div class="content-value">		<div class="order-head pos-rel">			<h1>                {% helper seo.seotag('header_text') %}:&nbsp;                #{{ orders.code }}&nbsp;({{ orders.status_text }})            </h1>			<span class="pos-abs link-order_log pointer">                {% helper lang.l('link_order_log', 'store') %}            </span>		</div>	</div></div><div class="clr"></div><script>    $(function(){        loadScripts(            "{% helper utils.jscript('store', 'store_orders.js', 'path') %}",            function(){                orders = new storeOrders({                    siteUrl: site_url,                    order: {id_order: '{{ orders.id }}'},                });            },            'orders',            {async: true}        );    });</script>{% include '@app/footer.twig' %}
