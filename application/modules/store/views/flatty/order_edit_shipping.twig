<div class="content-block load_content">    <div class="title-block">{% helper lang.l('field_fill_shipping_address', 'store') %}</div>    <div class="edit-address-block clearfix">        <input type="hidden" name="address_form_id" value="{{ address.id }}">        <div class="country-select form-group">            <label class="col-xs-12 tali no-padding-left" for="country_name">                {% helper lang.l('field_country', 'store') %}            </label>            <div class="col-xs-6 no-padding-left">                <div class="btn-group" id="country_load">                    <button type="button" class="btn btn-default col-xs-10 col-sm-10 col-lg-11" id="country_name">                        {% if address.country %}{{ address.country }}{% else %}{% helper lang.l('link_select_countries', 'store', '', 'button') %}{% endif %}                    </button>                    <button type="button" class="btn btn-default dropdown-toggle col-xs-2 col-sm-2 col-lg-1" data-toggle="dropdown">                            <span class="caret"></span>                     </button>                </div>                 <input type="hidden" name="country_id" id="id_hidden_country" value="{{ address.country_code }}">                <div class="col-xs-10 col-sm-10 col-lg-11 no-padding-left">                    <div id="country_box" class="dropdown">                        <ul></ul>                    </div>                                        </div>			</div>            <div class="alert alert-danger error hide col-xs-6" id="error-country"></div>		</div>        <div class="region-select form-group">            <label for="region" class="col-xs-12 tali no-padding-left">                <span id="region_lang">                    {% helper lang.l('field_region', 'store') %}:                </span>            </label>            <div class="col-xs-6 no-padding-left">                <input type="text" id="region_name" name="region_name" class="form-control" value="{{ address.region|e }}" placeholder="{% helper lang.l('field_placeholder', 'store', '', 'button') %}">                <input type="hidden" name="region_id" id="id_hidden_region" value="{{ address.region_id }}">                  <div class="col-xs-12 no-padding-left">                    <div id="region_box" class="dropdown clearfix">                        <ul></ul>                    </div>                                    </div>            </div>            <div class="alert alert-danger error hide col-xs-6" id="error-region"></div>                    </div>        <div class="city-select form-group">            <label for="city" class="col-xs-12 tali no-padding-left">                <span id="city_lang">                    {% helper lang.l('field_city', 'store') %}:                </span>            </label>            <div class="col-xs-6 no-padding-left">                <input type="text" id="city_name" name="city_name" class="form-control" value="{{ address.city|e }}" placeholder="{% helper lang.l('field_placeholder', 'store', '', 'button') %}">                <input type="hidden" name="city_id" id="id_hidden_city" value="{{ address.city_id }}">                <div class="col-xs-12 no-padding-left">                    <div id="city_box" class="dropdown clearfix">                        <ul></ul>                    </div>                                    </div>            </div>               <div class="alert alert-danger error hide col-xs-6" id="error-city"></div>        </div>        <div class="form-group">            <label class="col-xs-12 tali no-padding-left" for="street_address">                {% helper lang.l('field_street_address', 'store') %}:            </label>            <div class="col-xs-6 no-padding-left">                <input type="text" id="street_address" name="street_address" class="form-control" value="{{ address.address|e }}">            </div>            <div class="alert alert-danger error hide col-xs-6" id="error-address"></div>        </div>        <div class="form-group">            <label class="col-xs-12 tali no-padding-left" for="phone">                {% helper lang.l('field_phone', 'store') %}:            </label>            <div class="col-xs-6 no-padding-left">                <input type="tel" id="phone" name="phone" class="form-control" value="{{ address.phone|e }}">            </div>            <div class="alert alert-danger error hide col-xs-6" id="error-phone"></div>        </div>        <div class="form-group">            <label class="col-xs-12 tali no-padding-left" for="zip">                {% helper lang.l('field_zip', 'store') %}:            </label>            <div class="col-xs-6 no-padding-left">                <input type="text" id="zip" name="zip"  class="form-control" value="{{ address.zip|e }}">            </div>            <div class="alert alert-danger error hide col-xs-6" id="error-zip"></div>        </div>        <div class="form-group">            <label class="col-xs-12 tali no-padding-left" for="comment">                {% helper lang.l('field_comment', 'store') %}:            </label>            <div class="col-xs-6 no-padding-left">                <textarea id="comment" name="comment"  class="form-control"></textarea>            </div>            <div class="alert alert-danger error hide col-xs-6" id="error-comment"></div>        </div>        <div class="form-group">            <div class="button-block">                <div class="col-xs-12 no-padding-left">                    <input type="button" name="btn_save" id="save_address" data-addressid="{{ address.id }}" value="{% helper lang.l('btn_save_address', 'store', '', 'button') %}" class="btn btn-primary">                    <div class="cancel btn btn-cancel">                        {% helper lang.l('btn_cancel', 'start') %}                    </div>                </div>            </div>        </div>    </div></div>
